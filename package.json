{
  "name": "persona5-items",
  "version": "1.4.6",
  "private": true,
  "standard": {
    "env": [
      "jest"
    ]
  },
  "homepage": "https://quan.github.io/persona5-items",
  "repository": {
    "type": "git",
    "url": "https://github.com/quan/persona5-items.git"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "jquery": "^3.2.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "sticky-table-headers": "^0.1.19"
  },
  "devDependencies": {
    "enzyme": "^2.9.1",
    "gh-pages": "^1.0.0",
    "react-scripts": "1.0.7",
    "react-test-renderer": "^15.6.1",
    "sass": "^1.0.0-beta.2",
    "sinon": "^2.4.1",
    "standard": "^10.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "npm run build:sass",
    "build": "react-scripts build",
    "build:sass": "./node_modules/.bin/sass ./src/css/index.scss > ./src/css/index.css",
    "pretest": "npm run test:linter",
    "test": "react-scripts test --env=jsdom",
    "test:linter": "./node_modules/.bin/standard --fix",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  }
}
